{

  "description": "Imprime dados quando acionado",
  "manifest_version": 2,
  "name": "ImprimeDados",
  "version": "1.0",
  "icons": {
    "48": "icons/beasts-48.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/beasts-32.png",
    "default_title": "Imprimir",
    "default_popup": "popup/popup.html"
  }

}