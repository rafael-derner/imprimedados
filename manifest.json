{
    "manifest_version": 2,
    "name": "Imprime Dados",
    "version": "1.0",

    "description": "Imprime dados na tela",

    "content_scripts": [
        {
            "matches": ["*://*.google.com/*"],
            "js": ["imprimedados.js"]
        }
    ],

    "permissions": [
        "activeTab",
        "*://*.google.com/*"
    ]
}